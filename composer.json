{
    "name": "yiisoft/yii-swiftmailer",
    "description": "The SwiftMailer integration for the Yii framework",
    "keywords": ["yii", "swift", "swiftmailer", "mail", "email", "mailer"],
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-swiftmailer/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-swiftmailer"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/core": "^3.0.0",
        "swiftmailer/swiftmailer": "~6.0"
    },
    "autoload": {
        "psr-4": {"yii\\swiftmailer\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\swiftmailer\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
